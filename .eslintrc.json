{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "project": ["./tsconfig.json"],
        "sourceType": "module"
    },
    "rules": {
        "@typescript-eslint/no-namespace": "off",
        "curly": ["error", "multi", "consistent"]
    }
}
